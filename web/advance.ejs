<html>
   <head>
      <% include inc/head %>
      <title>ACTIVITIES - SCHEDULELAND</title>
   </head>
   </head>
   <body class="activities-page">
<style>  
@import url('http://cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2.css');
@import url('http://cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2-bootstrap.css');
@import url('http://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker.min.css');
.timepicker-picker .table-condensed td{text-align:center}                                              
</style>

<script>
//Select2
$.getScript('http://cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2.min.js',function(){
           
  /* dropdown and filter select */
  var select = $('#select2').select2();
  
  /* Select2 plugin as tagpicker */
  $("#tagPicker").select2({
    closeOnSelect:false
  });

}); //script         
      
  
//Date picker
$.getScript('//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.min.js',function(){
  
  $('#datePicker').datepicker({
    autoclose:true,
  }).on("changeDate", function(e){
    console.log(e.date);
  });
  
  $('.input-daterange').datepicker({
    autoclose:true
  }).on("changeDate", function(e){
    console.log(e.date);
  });
  
}); //script  
   


$.getScript('//cdnjs.cloudflare.com/ajax/libs/moment.js/2.6.0/moment.min.js',function(){
  $.getScript('//cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/3.0.0/js/bootstrap-datetimepicker.min.js',function(){
    
    $('#timePicker').datetimepicker({
      pickDate: false
    });
	
	    $('#timePicker2').datetimepicker({
      pickDate: false
    });
    
  }); //script
}); //script
</script>

   
      <!-- Fixed navbar -->
      <% include inc/navbar_auth.ejs %>
      <div class="container site-container">
	  
	  <div class="container-fluid">
  <h3>Advanced Searching</h3>
</div>
<hr>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
    	<h4>Number of people</h4>
      	<select class="form-control" id="select2">
                 <option value="Not sure">Not sure</option>					
                 <option value="1-2">1-2</option>
                 <option value="3-4">3-4</option>
                 <option value="5-8">5-8</option>
                 <option value="9-12">9-12</option>                 
				 <option value=">12">>12</option>	
        </select>
    </div> 
 
  </div><!--/row-->
  <hr>
    <div class="col-sm-3">
      <h4>Time Range Selector</h4>
      <div class="form-group">
        <label class="control-label">From</label>
        <div class="input-group" id="timePicker">
          <input class="form-control tFrom" type="text">
          <span class="input-group-addon"><span class="glyphicon glyphicon-time"></span></span>
        </div>
      </div>
    </div><!--/col-->
    <div class="col-sm-3">
      <h4>&nbsp;</h4>
      <div class="form-group">
        <label class="control-label">To</label>
        <div class="input-group" id="timePicker2">
          <input class="form-control tTo" type="text">
          <span class="input-group-addon"><span class="glyphicon glyphicon-time"></span></span>
        </div>
      </div>
    </div><!--/col-->
    <div class="col-sm-6">
      <h4>Date Range Selector</h4>
      <div class="row">
        <div class="col-sm-6">
          <div class="input-daterange form-group has-feedback">
            <label class="control-label">Start</label>
            <input name="dateStart" class="form-control dstart" type="text" placeholder="from">
            <span class="glyphicon glyphicon-calendar form-control-feedback"></span>
          </div>
        </div><!--/col-->
        <div class="col-sm-6">
          <div class="input-daterange form-group has-feedback">
            <label class="control-label">End</label>
            <input name="dateEnd" class="form-control dend" type="text" placeholder="to">
            <span class="glyphicon glyphicon-calendar form-control-feedback"></span>
          </div>
        </div><!--/col-->
      </div><!--/row-->
      
    </div><!--/col-->
  </div><!--/row-->
          <div class="col-md-12">
               <div id="custom-search-input">
                  <div class="input-group col-md-12">
                     <input type="text" class="form-control inputKeyword" placeholder="Enter some keywords.." />
                     <span class="input-group-btn">
                     <button type="submit" class="btn btn-default btn-asearch">
                     <span class="glyphicon glyphicon-search"></span>
                     </button>
                     </span>
                  </div>
               </div>
               <!-- /input-group -->
            </div>

</div>
	  

         <hr>


      </div>
      <!--footer-->
      <% include inc/footer %>
   </body>
</html>